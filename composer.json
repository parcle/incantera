{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "3.5.3",
		"silverstripe/framework": "3.5.3",
		"silverstripe/reports": "3.5.3",
		"silverstripe/siteconfig": "3.5.3",
		"silverstripe-themes/simple": "3.1.*",
		"wilr/silverstripe-facebookconnect": "dev-master",
		"guzzlehttp/guzzle": "dev-master",
		"facebook/php-sdk-v4": "@stable",
		"php-ffmpeg/php-ffmpeg": "~0.5",
		"b-b3rn4rd/phplame": "dev-master",
		"phpmailer/phpmailer": "~5.2",
		"pelago/emogrifier": "@dev"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.5.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
